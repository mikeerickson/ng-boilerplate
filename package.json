    "karma-mocha": "^0.2.1",
    "karma-mocha-clean-reporter": "https://registry.npmjs.org/karma-mocha-clean-reporter/-/karma-mocha-clean-reporter-0.0.1.tgz",
    "karma-mocha-reporter": "1.2.0",
    "karma-phantomjs2-launcher": "0.5.0",
