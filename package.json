{
  "name": "ng-boilerplate",
  "version": "1.0.0",
  "description": "Simple Angular Boilerplate",
  "main": "",
  "scripts": {
    "test": "mocha specs",
    "test2": "mocha specs --reporter mocha-unfunk-reporter",
    "reporter-install": "npm i -D mocha-unfunk-reporter",
    "reporter-url": "https://github.com/mikeerickson/mocha-unfunk-reporter",
    "mocha-html": "mocha specs --reporter mocha-html-reporter | cat node_modules/mocha-html-reporter/docs/head.html - node_modules/mocha-html-reporter/docs/tail.html  > spec-runner.html",
    "mocha-html2": "mocha specs --reporter better-mocha-html-reporter > better.html && open better.html",
    "serve": "json-server -w data/db.json"
  },
  "keywords": [
    "angular",
    "boilerplate",
    "gulp",
    "yeoman"
  ],
  "author": "Michael S Erickson",
  "license": "MIT",
  "dependencies": {
    "angular": "1.5.0",
    "angular-animate": "1.5.0",
    "angular-gravatar": "0.4.1",
    "angular-mocks": "1.5.0",
    "angular-ui-bootstrap": "^0.14.3",
    "app-root-path": "1.0.0",
    "bootstrap": "^3.3.5",
    "del": "^2.0.2",
    "grunt-cli": "^0.1.13",
    "gulp": "^3.9.0",
    "jquery": "^2.1.4",
    "jshint-summary": "^0.4.0",
    "lodash": "4.5.0",
    "moment": "^2.10.6",
    "ng-describe": "^1.4.1",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.4",
    "sinon-chai": "2.8.0"
  },
  "devDependencies": {
    "angular-mocks": "1.4.9",
    "bardjs": "0.1.8",
    "chai": "^3.4.1",
    "chai-as-promised": "5.2.0",
    "chalk": "^1.1.1",
    "chalkline": "0.0.4",
    "gulp-concat": "^2.6.0",
    "gulp-copy": "0.0.2",
    "gulp-eslint": "2.0.0",
    "gulp-ignore": "^2.0.1",
    "gulp-jshint": "1.12.0",
    "gulp-messenger": "^0.12.0",
    "gulp-mocha": "^2.2.0",
    "gulp-notify": "2.2.0",
